.home-container {
  background-image: url('../../../assets/images/japan1.jpg');
  height: calc(100vh - 60px);
}

.home-content {
  background: #fff;
  height: 100%;
  margin: 0 auto;
  padding: 20px;
  max-width: 980px;
}

.built-with-section-container {
  align-items: center;
  display: grid;
  grid-template-columns: repeat(auto-fill, 25%);
  height: 100px;
  justify-items: center;
  padding-top: 20px;

  & img {
    width: 100px;
  }
}
